{
  "id": "https://w3id.org/class/redaktor/PropertyValue",
  "type": "Concept",
  "followers": "https://test.skohub.io/followers?subject=vocab%2F%2Fw3id.org%2Fclass%2Fredaktor%2FPropertyValue",
